<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.gfsnutrition" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Battery">
        <param name="android-package" value="org.apache.cordova.batterystatus.BatteryListener" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <feature name="Globalization">
        <param name="android-package" value="org.apache.cordova.globalization.Globalization" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="Capture">
        <param name="android-package" value="org.apache.cordova.mediacapture.Capture" />
    </feature>
    <feature name="NativeStorage">
        <param name="android-package" value="NativeStorage" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="Vibration">
        <param name="android-package" value="org.apache.cordova.vibration.Vibration" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="android-package" value="com.phonegap.plugins.barcodescanner.BarcodeScanner" />
    </feature>
    <feature name="FirebaseAnalytics">
        <param name="android-package" value="by.chemerisuk.cordova.firebase.FirebaseAnalyticsPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <name>GFS Nutrition Facts</name>
    <description>
        GFS Nutrition Facts is a barcode scanner app that specializes in giving the user details on their food!
    </description>
    <author email="cezimmerman90@gmail.com" href="https://gfs.com">
        Chuck Zimmerman
    </author>
    <content src="index.html" />
    <access origin="*" />
    <gap:plugin name="cordova-plugin-x-socialsharing" source="npm" />
    <icon density="mdpi" src="www/res/icon/android/mipmap-mdpi/gfs_nutrition_laucher.png" />
    <icon density="hdpi" src="www/res/icon/android/mipmap-hdpi/gfs_nutrition_laucher.png" />
    <icon density="xhdpi" src="www/res/icon/android/mipmap-xhdpi/gfs_nutrition_laucher.png" />
    <icon density="xxhdpi" src="www/res/icon/android/mipmap-xxhdpi/gfs_nutrition_laucher.png" />
    <icon density="xxxhdpi" src="www/res/icon/android/mipmap-xxxhdpi/gfs_nutrition_laucher.png" />
    <splash density="land-ldpi" src="www/res/screen/android/barcode-512.png" />
    <splash density="land-mdpi" src="www/res/screen/android/barcode-512.png" />
    <splash density="land-hdpi" src="www/res/screen/android/barcode-512.png" />
    <splash density="land-xhdpi" src="www/res/screen/android/barcode-512.png" />
    <splash density="land-xxhdpi" src="www/res/screen/android/barcode-512.png" />
    <splash density="land-xxxhdpi" src="www/res/screen/android/barcode-512.png" />
    <splash density="port-ldpi" src="www/res/screen/android/barcode-512.png" />
    <splash density="port-mdpi" src="www/res/screen/android/barcode-512.png" />
    <splash density="port-hdpi" src="www/res/screen/android/barcode-512.png" />
    <splash density="port-xhdpi" src="www/res/screen/android/barcode-512.png" />
    <splash density="port-xxhdpi" src="www/res/screen/android/barcode-512.png" />
    <splash density="port-xxxhdpi" src="www/res/screen/android/barcode-512.png" />
    <uses-permission name="android.permission.INTERNET" />
    <uses-permission name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission name="android.permission.CAMERA" />
    <uses-permission name="android.permission.RECORD_AUDIO" />
    <allow-intent href="market:*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="orientation" value="portrait" />
</widget>
